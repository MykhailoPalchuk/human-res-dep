<Window x:Class="Views.ChooseProject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Views"
        mc:Ignorable="d"
        Title="Choose project" Height="300" Width="300">
    <Grid KeyDown="Grid_KeyDown">
        <Button Name="OK" Content="OK" FontSize="15" Height="30" Width="60" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="20" Click="OK_Click"/>
        <Button Name="Cancel" Content="Cancel" FontSize="15" Height="30" Width="60" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,212,20" Click="Cancel_Click"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Center" FontSize="15" Margin="0" VerticalAlignment="Center" Width="130" Height="30"/>
    </Grid>
</Window>
